<% layout('./layouts/boilerplate.ejs')%>
<style>
  .elementor-background-overlay{
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, 0.5); /* Semi-transparent black overlay */
    z-index: 1; /* Ensures the overlay is above the background but below the content */

}
.bg-primary {
      --tw-bg-opacity: 1;
      background-color: rgb(0 153 204 / var(--tw-bg-opacity));
  }

.bg-secondary{
  background-color: gainsboro;
}
@keyframes fadeIn {
  from { opacity: 0; transform: translateY(20px); }
  to { opacity: 1; transform: translateY(0); }
}

.text-animation {
  animation: fadeIn 2s ease-in-out forwards;
}

/* Set the initial background image */
#home {
  background-image: url('imgs/awotitle-inner-harbor-of-baltimore-maryland-xl.jpg');
}
.whitec{
  color: white;
}
.grayc{
  color: rgba(255, 255, 255, 0.548);
}

/* CSS for animated background transition */
@keyframes changeBackground {
  0%, 20% { background-image: url('imgs/awotitle-inner-harbor-of-baltimore-maryland-xl.jpg'); }
  25%, 45% { background-image: url('imgs/awotitle-classic-american-arquitecture-in-washington-dc-l.jpg'); } /* Replace with your second image path */
  50%, 70% { background-image: url('imgs/awotitle-inner-harbor-of-baltimore-maryland-xl.jpg'); } /* Replace with your third image path */
  75%, 95% { background-image: url('imgs/awotitle-classic-american-arquitecture-in-washington-dc-l.jpg'); }
  100% { background-image: url('imgs/awotitle-inner-harbor-of-baltimore-maryland-xl.jpg'); }
}

/* Apply the animation to the home container */
#home {
  animation: changeBackground 20s infinite;
}
.text-animation {
  opacity: 0;
  animation: fadeIn 2s ease-in-out forwards;
}
.sdssds:hover{
  background-color: white;
  color: black;
}

